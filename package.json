{
  "name": "sbike",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "lint": "eslint --ext '.js,.ts,.tsx' .",
    "typescript": "tsc --noEmit",
