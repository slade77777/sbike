    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "typescript": "^3.9.5"
  },
  "resolutions": {
    "react": "~16.13.1",
    "react-native": "~0.62.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.6",
    "axios": "^0.19.2",
    "dayjs": "^1.8.33",
    "dotenv": "^8.2.0",
    "patch-package": "^6.2.2",
    "postinstall-postinstall": "^2.1.0",
    "qs": "^6.9.4",
    "react": "16.9.0",
    "react-dom": "^16.13.1",
    "react-native-web": "^0.12.3"
